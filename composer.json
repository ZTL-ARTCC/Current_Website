{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-timezone.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/html.git"
        }
    ],
    "require": {
        "php": "^8.3",
        "devmarketer/easynav": "^1.0",
        "guzzlehttp/guzzle": "^7.8",
        "jamesmills/laravel-timezone": "dev-l11-compatibility",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.7",
        "laravel/legacy-factories": "^1.4.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5",
        "spatie/laravel-html": "^3.7",
        "league/oauth2-client": "^2.7",
        "maatwebsite/excel": "^3.1",
        "mitoteam/jpgraph": "^10.2",
        "psr/simple-cache": "^2.0",
        "santigarcor/laratrust": "^8.0",
        "spatie/laravel-schedule-monitor": "^3.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.14",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.5|^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",          
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=laratrust-assets --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "format-check": [
            "vendor/bin/php-cs-fixer fix --dry-run"
        ],
        "setup-git-hooks": [
            "cp .git-hooks/* .git/hooks/."
        ],
        "reset": [
            "@php artisan db:wipe", "@php artisan migrate", "@php artisan db:seed"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
